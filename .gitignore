cat > .gitignore <<'EOF'
# dependencies
node_modules/

# build outputs
dist/
out/
release/
build/

# logs
*.log
logs/

# env / secrets
.env
.env.*
!.env.example

# OS / IDE
.DS_Store
.vscode/
.idea/

# misc
npm-debug.log*
yarn-debug.log*
yarn-error.log*
EOF

# local dumps / tools
dump_*.txt
make_dump.py
